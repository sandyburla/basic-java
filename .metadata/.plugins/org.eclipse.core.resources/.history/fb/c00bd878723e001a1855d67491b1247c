package com.reviews.tripadvisor.model;

public class Data {
	    private String id;

	    private String lang;

	    private String location_id;

	    private String published_date;

	    private int rating;

	    private String helpful_votes;

	    private String rating_image_url;

	    private String url;

	    private String trip_type;

	    private String travel_date;

	    private String text;

	    private User user;

	    private String title;

	    private Owner_response owner_response;

	    private List<String> subratings;

	    public void setId(String id){
	        this.id = id;
	    }
	    public String getId(){
	        return this.id;
	    }
	    public void setLang(String lang){
	        this.lang = lang;
	    }
	    public String getLang(){
	        return this.lang;
	    }
	    public void setLocation_id(String location_id){
	        this.location_id = location_id;
	    }
	    public String getLocation_id(){
	        return this.location_id;
	    }
	    public void setPublished_date(String published_date){
	        this.published_date = published_date;
	    }
	    public String getPublished_date(){
	        return this.published_date;
	    }
	    public void setRating(int rating){
	        this.rating = rating;
	    }
	    public int getRating(){
	        return this.rating;
	    }
	    public void setHelpful_votes(String helpful_votes){
	        this.helpful_votes = helpful_votes;
	    }
	    public String getHelpful_votes(){
	        return this.helpful_votes;
	    }
	    public void setRating_image_url(String rating_image_url){
	        this.rating_image_url = rating_image_url;
	    }
	    public String getRating_image_url(){
	        return this.rating_image_url;
	    }
	    public void setUrl(String url){
	        this.url = url;
	    }
	    public String getUrl(){
	        return this.url;
	    }
	    public void setTrip_type(String trip_type){
	        this.trip_type = trip_type;
	    }
	    public String getTrip_type(){
	        return this.trip_type;
	    }
	    public void setTravel_date(String travel_date){
	        this.travel_date = travel_date;
	    }
	    public String getTravel_date(){
	        return this.travel_date;
	    }
	    public void setText(String text){
	        this.text = text;
	    }
	    public String getText(){
	        return this.text;
	    }
	    public void setUser(User user){
	        this.user = user;
	    }
	    public User getUser(){
	        return this.user;
	    }
	    public void setTitle(String title){
	        this.title = title;
	    }
	    public String getTitle(){
	        return this.title;
	    }
	    public void setOwner_response(Owner_response owner_response){
	        this.owner_response = owner_response;
	    }
	    public Owner_response getOwner_response(){
	        return this.owner_response;
	    }
	    public void setSubratings(List<String> subratings){
	        this.subratings = subratings;
	    }
	    public List<String> getSubratings(){
	        return this.subratings;
	    }
}
